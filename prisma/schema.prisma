generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:dev.db"
}

model Event {
  id          Int      @id @default(autoincrement())
  title       String
  description String?
  start_time  DateTime
  end_time    DateTime
  date        DateTime
  created_at  DateTime @default(now())
  updated_at  DateTime @updatedAt
}
